{
  "name": "locaz",
  "version": "1.0.0",
  "description": "API para o sistema Locaz",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/app.js",
    "start:dev": "nodemon --exec ts-node -r dotenv/config src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -d ./data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/data-source.ts ./src/migrations/",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./src/data-source.ts",
    "typeorm:generate-migration": "npm run typeorm migration:generate -- ./src/migration/CrianTabelas"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "streamifier": "^0.1.1",
    "typeorm": "^0.3.26",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.25",
    "@types/streamifier": "^0.1.2",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
